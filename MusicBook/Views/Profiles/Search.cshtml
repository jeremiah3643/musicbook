@model IEnumerable<MusicBook.Models.ApplicationUser>
    @{
        ViewData["Title"] = "Search";
    }

    <h2>Search Results</h2>

    <div>
        

    </div>
    <div class="card" style="width: 18rem;">
        <div class="card-body">
           
            @foreach (var item in Model)
            {
            <div class="card-title">@Html.DisplayFor(modelItem => item.UserName)</div>
            <a class="btn btn-primary" asp-action="Details" asp-route-id="@item.Id">View Profile</a> 
            }
        </div>
    </div>